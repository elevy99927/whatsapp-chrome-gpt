 {
  "manifest_version": 2,
  "name": "WhatsApp GPT Connector",
  "version": "0.0.1",
  "permissions": [
    "activeTab",
    "storage",
    "https://web.whatsapp.com/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://web.whatsapp.com/*"],
      "js": ["content.js"]
    }
  ]
}
